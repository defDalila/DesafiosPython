    "def verificador_ano_bissexto():\n",
    "    ano = int(input())\n",
    "    \n",
    "    regra_um = (ano % 4 == 0) and (ano % 100 != 0)\n",
    "    regra_dois = ano % 400 == 0\n",
    "    \n",
    "    if (regra_um or regra_dois):\n",
    "        print(\"SIM\")\n",
    "    else:\n",
    "        print(\"NÃO\")\n"
